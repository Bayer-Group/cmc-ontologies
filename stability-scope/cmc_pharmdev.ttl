@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dc:   <http://purl.org/dc/elements/1.1/> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix pharmdev:   <https://github.com/Bayer-Group/cmc-ontologies/70107/001/> .
@prefix solont: <https://github.com/Bayer-Group/cmc-ontologies/2000/5/> .
@prefix prov: <http://www.w3.org/TR/prov-o/> .
@prefix main: <https://github.com/Bayer-Group/cmc-ontologies/kos/19014/> .

# This ontology serves as basis for general concepts like: substance, substanceID, batch, batchID, material, materialID, etc.
# Its context arose from Pharmaceutical Development and is used as auxiliary in Chemical Manufacturing and Control

# Ontology name: cmc_pharmdev , Abbreviation meaning Chemical Manufacturing and Control Pharmaceutical Development
# Ontology expressivity level: formost RDF/S only. OWL used at very minimum
# Ontology date: July 2024
# Ver. 1.0 
# Authors: this is a snippet of a wider vocabulary as developed at Bayer by few ontology engineers in different discussions.
# Final version  author: O. Streibel
# Changes for public availability: (re-definition, anonymization, dettach from systems URIs, replace properties by publicly available ontologies)

pharmdev:    a                     owl:Ontology ;
        rdfs:comment               "The general ontology on pharmaceutical development and pharmaceutical research related concepts. " ;
        rdfs:label                 "pharmdev ontology" ;
        owl:versionIRI       <https://github.com/Bayer-Group/cmc-ontologies/stability-scope/cmc_pharmdev.ttl> ;
        rdfs:prefLabel         "pharmdev ontology"@en ;
        dc:identifier        "pharmdev" ;
        dc:title      "CMC Pharmdev Ontology" ;
        owl:versionInfo "1.0" .
        
      
pharmdev:Batch rdf:type owl:Class ;
          rdfs:label "Batch"@en ;
            rdfs:subClassOf owl:Thing ;
          skos:definition "Units of a product made as specified groups or amounts, within a time frame at a specific physical location. Typically identified by a lot number, an identification number assigned to a particular quantity or lot of material."@en ;
          skos:example "Production lot 12345 produced on production line A in plant B at site C on June 11, 2019, containing D units of product E in packaging type F." ;
          prov:wasDerivedFrom "https://en.wikipedia.org/wiki/Batch_production" .

pharmdev:hasBatchID  a    owl:ObjectProperty ;
        rdfs:comment    "Links a batch to its identifier."@en ;
        rdfs:domain     pharmdev:Batch ;
        rdfs:label      "has batch ID"@en ;
        rdfs:range      pharmdev:BatchID ;
        skos:definition  "Links a batch to its identifier."@en .

pharmdev:BatchID  a        owl:Class ;
        rdfs:comment     "Identifier for a batch"@en ;
        rdfs:label       "Batch ID"@en ;
        rdfs:subClassOf  main:Identifier ;
        skos:definition   "Identifier for a batch"@en .

pharmdev:containsSolubilityResults a owl:ObjectProperty ;
    rdfs:comment "Links an Internal Number to its all solubility results."@en ;
    rdfs:label "contains solubility results"@en ;
    rdfs:domain pharmdev:InternalID ;
    rdfs:range solont:SolubilityResult .


pharmdev:Symptom  a              owl:Class ;
        rdfs:comment        "Subjective evidence of disease perceived by the patient."@en ;
        rdfs:label          "Symptom"@en , "Síntoma"@es , "Symptôme"@fr , "Symptom"@de , "Sintomo"@it , "Objaw"@pl ;
        rdfs:subClassOf     owl:Thing ;
        skos:definition      "Subjective evidence of disease perceived by the patient."@en ;
        skos:prefLabel  "Symptom"@en ;
        prov:wasDerivedFrom  <http://purl.bioontology.org/ontology/NCIT#C4876> .

pharmdev:hasTreatement  a        owl:ObjectProperty ;
        rdfs:comment        "Links a protocol with a treatment."@en ;
        rdfs:domain         pharmdev:Protocol ;
        rdfs:label          "a un traitement"@fr , "ha un trattamento"@it , "tiene tratamiento"@es , "ma Leczenie"@pl , "治療を受けている"@jp , "hat Behandlung"@de ;
        rdfs:range          pharmdev:Treatment ;
        skos:definition      "Links a protocol with a treatment."@en ;
        skos:prefLabel  "has Treatment"@en .

pharmdev:Treatment  a            owl:Class ;
        rdfs:comment        "A process in which the act is intended to modify or alter some other material entity."@en ;
        rdfs:label          "Treatment"@en , "Trattamento"@it , "Tratamiento"@es , "Traitement"@fr , "Leczenie"@pl , "Behandlung"@de ;
        rdfs:subClassOf     owl:Thing ;
        skos:definition      "A process in which the act is intended to modify or alter some other material entity."@en ;
        skos:prefLabel  "Treatment"@en ;
        prov:wasDerivedFrom  <http://www.ebi.ac.uk/efo/EFO_0000727> .

pharmdev:Protocol  a             owl:Class ;
        rdfs:comment        "A plan specification which has sufficient level of detail and quantitative information to communicate it between investigation agents, so that different investigation agents will reliably be able to independently reproduce the process."@en ;
        rdfs:label          "Protocol"@en , "Protokól"@pl , "Protokol"@de , "Protocolo"@es , "Protocollo"@it , "Protocole"@fr ;
        rdfs:subClassOf     main:Document ;
        rdfs:altLabel        "Study Protocol"@en ;
        skos:definition      "A plan specification which has sufficient level of detail and quantitative information to communicate it between investigation agents, so that different investigation agents will reliably be able to independently reproduce the process."@en ;
        skos:prefLabel  "Protocol"@en ;
        prov:wasDerivedFrom  <http://purl.bioontology.org/ontology/OBI/OBI_0000272> .

pharmdev:isTargetAssociatedWithDisease
        a                   owl:ObjectProperty ;
        rdfs:comment        "Links a target to a disease."@en ;
        rdfs:domain         pharmdev:Target ;
        rdfs:label          "è l'obiettivo associato alla malattia"@it , "est la cible associée à la maladie"@fr , "es el objetivo asociado con la enfermedad"@es , "ist Ziel assoziiert mit Krankheit"@de , "jest powiązany z chorobą"@pl , "病気に関連する標的"@jp , "is target associated with disease"@en ;
        rdfs:range          pharmdev:Disease ;
        skos:definition      "Links a target to a disease."@en ;
        skos:prefLabel  "is target associated with disease"@en .

pharmdev:Target  a               owl:Class ;
        rdfs:comment        "An object fixed as a goal or point of examination; something to point at; a destination."@en ;
        rdfs:label          "Target"@en , "Target"@de , "Objetivo"@es , "Obiettivo"@it , "Cible"@fr , "Cel"@pl ;
        rdfs:subClassOf     owl:Thing ;
        skos:definition      "An object fixed as a goal or point of examination; something to point at; a destination."@en ;
        skos:prefLabel  "Target"@en ;
        prov:wasDerivedFrom  <http://purl.bioontology.org/ontology/NCIT#C25702> .

pharmdev:Disease  a              owl:Class ;
        rdfs:comment        "A definite pathologic process with a characteristic set of signs and symptoms. It may affect the whole body or any of its parts, and its etiology, pathology, and prognosis may be known or unknown."@en ;
        rdfs:label          "Disease"@en , "Malattia"@it , "Maladie"@fr , "Krankheit"@de , "Enfermedad"@es , "Choroba"@pl ;
        rdfs:subClassOf     owl:Thing ;
        rdfs:altLabel        "Illness"@en , "Sickness"@en , "Ailment"@en ;
        skos:definition      "A definite pathologic process with a characteristic set of signs and symptoms. It may affect the whole body or any of its parts, and its etiology, pathology, and prognosis may be known or unknown."@en ;
        skos:prefLabel  "Disease"@en ;
        prov:wasDerivedFrom  <http://purl.bioontology.org/ontology/MESH/D004194> .

pharmdev:Protein  a              owl:Class ;
        rdfs:comment        "A group of complex organic macromolecules composed of one or more chains (linear polymers) of alpha-L-amino acids linked by peptide bonds and ranging in size from a few thousand to over 1 million Daltons. Proteins are fundamental genetically encoded components of living cells with specific structures and functions dictated by amino acid sequence."@en ;
        rdfs:label          "Protein"@en , "Protéine"@fr , "Proteína"@es , "Proteina"@it , "Protein"@de , "Bialko"@pl ;
        rdfs:seeAlso        <http://purl.bioontology.org/ontology/SNOMEDCT/88878007> ;
        rdfs:subClassOf     pharmdev:Target ;
        skos:definition      "A group of complex organic macromolecules composed of one or more chains (linear polymers) of alpha-L-amino acids linked by peptide bonds and ranging in size from a few thousand to over 1 million Daltons. Proteins are fundamental genetically encoded components of living cells with specific structures and functions dictated by amino acid sequence."@en ;
        skos:prefLabel  "Protein"@en ;
        prov:wasDerivedFrom  <http://purl.bioontology.org/ontology/NCIT#C17021> .

pharmdev:hasAssociatedSymptom
        a                   owl:ObjectProperty ;
        rdfs:comment        "Links a disease to a symptom."@en ;
        rdfs:domain         pharmdev:Disease ;
        rdfs:label          "związany z objawem"@pl , "ha Sintomo Associato"@it , "Tiene síntomas asociados"@es , "présente un symptôme associé"@fr , "hat assoziiertes Symptom"@de , "関連症状がある"@jp , "has associated symptom"@en ;
        rdfs:range          pharmdev:Symptom ;
        skos:definition      "Links a disease to a symptom."@en ;
        skos:prefLabel  "has associated symptom"@en .

pharmdev:Dose  a                 owl:Class ;
        rdfs:comment        "A quantity of an agent (such as substance or energy) administered, taken, or absorbed at one time."@en ;
        rdfs:label          "Dose"@en , "Dosis"@es , "Dosis"@de , "Dose"@it , "Dose"@fr , "Dawka"@pl ;
        rdfs:seeAlso        <http://www.ebi.ac.uk/efo/EFO_0000428> ;
        rdfs:subClassOf     owl:Thing ;
        skos:definition      "A quantity of an agent (such as substance or energy) administered, taken, or absorbed at one time."@en ;
        skos:prefLabel  "Dose"@en ;
        prov:wasDerivedFrom  <http://purl.bioontology.org/ontology/NCIT#C25488> .

pharmdev:PPN  a                  owl:Class ;
        rdfs:comment        "Abstract designator for a pharmaceutical product from a R&D perspective. A PPN does not provide any information about the drug dose form of a product. Rather, it groups the different variants of a product under a single designator."@en ;
        rdfs:label          "Product Principle Name"@en ;
        rdfs:subClassOf     owl:Thing ;
        rdfs:altLabel        "PPN"@en ;
        skos:definition      "Abstract designator for a pharmaceutical product from a R&D perspective. A PPN does not provide any information about the drug dose form of a product. Rather, it groups the different variants of a product under a single designator."@en ;
        skos:example         "Nexavar" , "Xarelto" , "Aspirin" ;
        skos:prefLabel  "Product Principle Name"@en .

pharmdev:hasInternalID  a         owl:ObjectProperty ;
        rdfs:comment        "Links a compound to its assigned Internal Number" ;
        rdfs:domain         pharmdev:Compound ;
        rdfs:label          "has Internal Number"@en ;
        rdfs:range          pharmdev:InternalID ;
        rdfs:subPropertyOf  pharmdev:hasCompoundID ;
        skos:definition      "Links a compound to its assigned Internal Number"@en ;
        skos:editorialNote  "Internal Numbers are not unique identifiers for compounds. Single compound be assigned to multiple Internal Numbers."@en .

pharmdev:Compound  a             owl:Class ;
        rdfs:comment        "A substance formed by chemical union of two or more elements or ingredients in definite proportion by weight."@en ;
        rdfs:label          "Compound"@en , "Zlozony"@pl , "Präparat"@de , "Compuesto"@es , "Composé"@fr , "Composto"@it ;
        rdfs:subClassOf     owl:Thing ;
        skos:definition      "A substance formed by chemical union of two or more elements or ingredients in definite proportion by weight."@en ;
        skos:editorialNote  "*: A distinction must be made between \"Single Substance\" and \"Substance Combination\". "@en ;
        skos:prefLabel  "Compound"@en ;
        prov:wasDerivedFrom  <http://purl.bioontology.org/ontology/NCIT#C43366> .

pharmdev:InternalID  a            owl:Class ;
        rdfs:comment        "Main identifier for verified and validated compounds within the Organization. Internal Numbers are assigned to single substances as well as to substance combinations."@en ;
        rdfs:label          "Internal Number"@en ;
        rdfs:subClassOf     pharmdev:CompoundID ;
        rdfs:altLabel        "Internal No."@en ;
        skos:definition      "Main identifier for verified and validated compounds within the Organization. Internal numbers are assigned to single substances as well as to substance combinations."@en ;
        skos:editorialNote  "Alternative spellings of an Internal Number and synonymous Internal Numbers (which should not exist in a perfect world) are recorded as alternative labels (rdfs:altLabel) of the valid Internal Number."@en , "Valid Internal numbers are intentionally captured as instances, so they have a URI. "@en ;
        skos:prefLabel  "Internal Number"@en .

pharmdev:hasCompoundID  a    owl:ObjectProperty ;
        rdfs:comment    "Links a compound to its identifier."@en ;
        rdfs:domain     pharmdev:Compound ;
        rdfs:label      "has compound ID"@en ;
        rdfs:range      pharmdev:CompoundID ;
        skos:definition  "Links a compound to its identifier."@en .

pharmdev:Pathway  a              owl:Class ;
        rdfs:comment        "A set or series of interactions, often forming a network, which biologists have found useful to group together for organizational, historic, biophysical, or other reasons."@en ;
        rdfs:label          "Pathway"@en , "Sciezka"@pl , "Sentiero"@it , "Sentier"@fr , "Pathway"@de , "Camino"@es ;
        rdfs:subClassOf     owl:Thing ;
        skos:definition      "A set or series of interactions, often forming a network, which biologists have found useful to group together for organizational, historic, biophysical, or other reasons."@en ;
        skos:prefLabel  "Pathway"@en ;
        prov:wasDerivedFrom  <http://purl.bioontology.org/ontology/NCIT#C54214> .

pharmdev:hasProtocol  a          owl:ObjectProperty ;
        rdfs:comment        "Links an experiment with its protocol."@en ;
        rdfs:domain         pharmdev:Experiment ;
        rdfs:label          "ma protokół"@pl , "ha protocollo"@it , "プロトコルを持つ"@jp , "a un protocole"@fr , "tiene protocolo"@es , "hat Protokoll"@de , "has protocol"@en ;
        rdfs:range          pharmdev:Protocol ;
        skos:definition      "Links an experiment with its protocol."@en ;
        skos:prefLabel  "has protocol"@en .

pharmdev:ResearchActivity  a     owl:Class ;
        rdfs:comment        "Systematic investigation into a subject in order to discover facts, establish or revise a theory, or develop a plan of action based on the facts discovered."@en ;
        rdfs:label          "Research Activity"@en , "Forschungsaktivität"@de , "Dzialalnosc badawcza"@pl , "Attività di ricerca"@it , "Activité de recherche"@fr , "Actividad de investigación"@es ;
        rdfs:subClassOf     main:Activity ;
        skos:definition      "Systematic investigation into a subject in order to discover facts, establish or revise a theory, or develop a plan of action based on the facts discovered."@en ;
        skos:prefLabel  "Research Activity"@en ;
        prov:wasDerivedFrom  <http://purl.bioontology.org/ontology/NCIT#C15429> .

pharmdev:Concentration  a        owl:Class ;
        rdfs:comment        "The quantity of a substance per unit volume or weight; a measure of the amount of substance present in a unit amount of mixture, particularly, the amount of solute dissolved in a solvent. The amounts can be expressed as moles, masses, volumes, or parts."@en ;
        rdfs:label          "Concentration"@en , "Stezenie"@pl , "Konzentration"@de , "Concentrazione"@it , "Concentration"@fr , "Concentración"@es ;
        rdfs:subClassOf     owl:Thing ;
        skos:definition      "The quantity of a substance per unit volume or weight; a measure of the amount of substance present in a unit amount of mixture, particularly, the amount of solute dissolved in a solvent. The amounts can be expressed as moles, masses, volumes, or parts."@en ;
        skos:prefLabel  "Concentration"@en ;
        prov:wasDerivedFrom  <http://purl.bioontology.org/ontology/NCIT#C41185> .

pharmdev:isSimilarDrugTo  a      owl:ObjectProperty , owl:TransitiveProperty ;
        rdfs:comment        "Links a drug to another drug it is similar to."@en ;
        rdfs:domain         pharmdev:Drug ;
        rdfs:label          "è simile alla droga"@it , "est un médicament similaire à"@fr , "jest podobny narkotyk"@pl , "ist eine ähnliche Droge"@de , "同様の薬物である"@jp , "es un medicamento similar a"@es , "is similar drug to"@en ;
        rdfs:range          pharmdev:Drug ;
        skos:definition      "Links a drug to another drug it is similar to."@en ;
        skos:editorialNote  "*: The following questions need to be answered: Under what conditions are two drugs considered similar? Who determines this? Where does one find the relevant guidelines?"@en ;
        skos:prefLabel  "is similar drug to"@en .

pharmdev:Drug  a                 owl:Class ;
        rdfs:comment        "Any substance which when absorbed into a living organism may modify one or more of its functions. The term is generally accepted for a substance taken for a therapeutic purpose, but is also commonly used for abused substances." ;
        rdfs:label          "Drug"@en , "Médicament"@fr , "Pharmazeutisches Mittel"@de , "Medikament"@de , "Farmaco"@it , "Medicamento"@es , "Drug"@pl ;
        rdfs:subClassOf     pharmdev:Compound ;
        skos:definition      "Any substance which when absorbed into a living organism may modify one or more of its functions. The term is generally accepted for a substance taken for a therapeutic purpose, but is also commonly used for abused substances."@en ;
        skos:prefLabel  "Drug"@en ;
        prov:wasDerivedFrom  <http://purl.obolibrary.org/obo/CHEBI_23888> .

pharmdev:hasActiveMoiety  a  owl:ObjectProperty ;
        rdfs:comment    "Links a compound to its active moiety which is also a compound."@en ;
        rdfs:domain     pharmdev:Compound ;
        rdfs:label      "has active moiety"@en ;
        rdfs:range      pharmdev:Compound ;
        skos:definition  "Links a compound to its active moiety which is also a compound."@en .

pharmdev:CompoundID  a        owl:Class ;
        rdfs:comment     "Identifier for a compound"@en ;
        rdfs:label       "Compound ID"@en ;
        rdfs:subClassOf  main:Identifier ;
        skos:definition   "Identifier for a compound"@en .

pharmdev:Experiment  a           owl:Class ;
        rdfs:comment        "A coordinated set of actions and observations designed to generate data, with the ultimate goal of discovery or hypothesis testing."@en ;
        rdfs:label          "Experiment"@en , "Sperimentare"@it , "Expérience"@fr , "Experimento"@es , "Experiment"@de , "Eksperyment"@pl ;
        rdfs:subClassOf     pharmdev:ResearchActivity ;
        skos:definition      "A coordinated set of actions and observations designed to generate data, with the ultimate goal of discovery or hypothesis testing."@en ;
        skos:prefLabel  "Experiment"@en ;
        prov:wasDerivedFrom  <http://purl.bioontology.org/ontology/NCIT#C42790> .

pharmdev:RegulatoryActivity
        a                   owl:Class ;
        rdfs:comment        "Activity concerning regulatory affairs."@en ;
        rdfs:label          "Regulatory Activity"@en ;
        rdfs:subClassOf     main:Activity ;
        rdfs:altLabel        "Event"@en ;
        skos:definition      "Activity concerning regulatory affairs."@en ;
        skos:example         "Submission"@en , "Variation"@en ;
        skos:prefLabel  "Regulatory Activity"@en ;
        skos:scopeNote       "Everything that happens to the application: sumbission - Variations, Amendments, Supplements, Annual Reports, and Renewals"@en .

pharmdev:hasVariation  a         owl:ObjectProperty ;
        rdfs:comment        "Links an authorized drug product with a variation/change that has been made on it."@en ;
        rdfs:domain         pharmdev:AuthorizedMedicinalProduct ;
        rdfs:label          "hat Variation"@de , "ha una variazione"@it , "tiene variación"@es , "ma Odmianę"@pl , "バリエーションがある"@jp , "a une variation"@fr , "has variation"@en ;
        rdfs:range          pharmdev:Variation ;
        skos:definition      "Links an authorized drug product with a variation/change that has been made on it."@en ;
        skos:prefLabel  "has variation"@en .

pharmdev:Variation  a            owl:Class ;
        rdfs:comment        "A change to an existing marketing authorization. For every change an updated dossier has to be provided to the local Health Authority. The approval is required to keep the marketing authorization valid."@en ;
        rdfs:label          "Variation"@en , "Zmiana"@pl , "Variazione"@it , "Variation"@fr , "Variation"@de , "Variación"@es ;
        rdfs:subClassOf     pharmdev:RegulatoryActivity ;
        rdfs:altLabel        "Change"@en ;
        skos:definition      "A change to an existing marketing authorization."@en ;
        skos:editorialNote  "Associated superclass \"Regulatory Activity\"."@en ;
        skos:example         "label change"@en ;
        skos:prefLabel  "Variation"@en .

pharmdev:AuthorizedMedicinalProduct
        a                   owl:Class ;
        rdfs:comment        "The medicinal product as it is licensed in a certain country to be marketed."@en ;
        rdfs:label          "Authorized Medicinal Product"@en , "Zatwierdzony lek"@pl , "Médicament approuvé"@fr , "Genehmigte Droge"@de , "Farmaco approvato"@it , "Droga aprobada"@es ;
        rdfs:subClassOf     pharmdev:PharmaceuticalProduct ;
        rdfs:altLabel        "Approved Drug"@en ;
        skos:definition      "The medicinal product as it is licensed in a certain country to be marketed."@en ;
        skos:editorialNote  "*: Insights gained during discussions with domain experts from Regulatory Affairs: The label for instances of class \"Authorized Medicinal Product\" should not be pre-concatenated (e.g. based on \"Product Principle Name and Form PPNF\", \"Country\" and \"Tradename\") to be used as a string in the label. Instead, the labels should be concatenated on the fly in the frontend of the application. This avoids the problem that the label must be recreated as soon as the label of a component changes (which, according to the domain experts, is not often, but regularly the case)."@en ;
        skos:prefLabel  "Authorized Medicinal Product"@en .

pharmdev:hasPPN  a                   owl:ObjectProperty ;
        rdfs:comment            "Links a Pharmaceutical Product to the assigned Product Principle Name (PPN)."@en ;
        rdfs:domain             pharmdev:PharmaceuticalProduct ;
        rdfs:label              "has Product Principle Name"@en ;
        rdfs:range              pharmdev:PPN ;
        rdfs:altLabel            "has PPN"@en ;
        skos:definition          "Links a Pharmaceutical Product to the assigned Product Principle Name (PPN)."@en ;
        skos:editorialNote      "Thanks to a Property Chain, the Product Principle Name (PPN) of the product can be inferred on the basis of the assigned Product Principle Name and Form (PPNF)."@en ;
        skos:prefLabel      "has Product Principle Name"@en ;
        owl:propertyChainAxiom  ( pharmdev:hasPPNF pharmdev:PPNFhasPPN ) .

pharmdev:PharmaceuticalProduct
        a                   owl:Class ;
        rdfs:comment        "Any substance introduced into a living organism with therapeutic or diagnostic purpose."@en ;
        rdfs:label          "Pharmaceutical Product"@en , "Produkt farmaceutyczny"@pl , "Produit pharmaceutique"@fr , "Producto farmacéutico"@es , "Prodotto farmaceutico"@it , "Pharmazeutisches Produkt"@de ;
        rdfs:subClassOf     pharmdev:Drug , main:Product ;
        rdfs:altLabel        "Medicament"@en ;
        skos:definition      "Any substance introduced into a living organism with therapeutic or diagnostic purpose."@en ;
        skos:prefLabel  "Pharmaceutical Product"@en ;
        prov:wasDerivedFrom  <http://purl.bioontology.org/ontology/CHEBI/CHEBI52217> .

pharmdev:SubstanceCombination
        a                  owl:Class ;
        rdfs:comment       "A combination of substances in distinction to compounds consisting of exactly one substance, called a \"Single Substance\"."@en ;
        rdfs:label         "Substance Combination"@en ;
        rdfs:subClassOf    pharmdev:Compound ;
        skos:definition     "A combination of substances in distinction to compounds consisting of exactly one substance, called a \"Single Substance\"."@en ;
        skos:example        "2,4-Dichlorobenzyl alcohol + L-Menthol + Amylmetacresol" , "Camphor + Eucalyptus globulus oil + Persea gratissima oil + Rose geranium oil + Rosa rubiginosa oil + Thyme essential oil" @en .

pharmdev:GeneExpression  a       owl:Class ;
        rdfs:comment        "The process in which a gene's sequence is converted into a mature gene product or products (proteins or RNA). This includes the production of an RNA transcript as well as any processing to produce a mature RNA product or an mRNA or circRNA (for protein-coding genes) and the translation of that mRNA or circRNA into protein. Protein maturation is included when required to form an active form of a product from an inactive precursor form."@en ;
        rdfs:label          "Gene Expression"@en , "L'expression du gène"@fr , "Genetische Ausprägung"@de , "Expresión genética"@es , "Ekspresja genu"@pl , "Espressione genica"@it ;
        rdfs:seeAlso        <http://purl.bioontology.org/ontology/MESH/D015870> , <http://purl.bioontology.org/ontology/NCIT#C16608> ;
        rdfs:subClassOf     owl:Thing ;
        skos:definition      "The process in which a gene's sequence is converted into a mature gene product or products (proteins or RNA). This includes the production of an RNA transcript as well as any processing to produce a mature RNA product or an mRNA or circRNA (for protein-coding genes) and the translation of that mRNA or circRNA into protein. Protein maturation is included when required to form an active form of a product from an inactive precursor form."@en ;
        skos:prefLabel  "Gene Expression"@en ;
        prov:wasDerivedFrom  <http://purl.obolibrary.org/obo/GO_0010467> .

pharmdev:PPNFhasPPN  a       owl:ObjectProperty ;
        rdfs:comment    "Assignment of the designator \"Product Principle Name\" (PPN) to the associated designator \"Product Principle Name and Form\" (PPNF). As a rule, several PPNFs can be assigned to one PPN. Since PPNF is more specific than PPN, there are usually several PPNF per PPN."@en ;
        rdfs:domain     pharmdev:PPNF ;
        rdfs:label      "PPNF has PPN"@en ;
        rdfs:range      pharmdev:PPN ;
        skos:definition  "Assignment of the designator \"Product Principle Name\" (PPN) to the associated designator \"Product Principle Name and Form\" (PPNF). As a rule, several PPNFs can be assigned to one PPN. Since PPNF is more specific than PPN, there are usually several PPNF per PPN."@en ;
        owl:inverseOf   pharmdev:PPNhasPPNF .

pharmdev:PPNF  a                 owl:Class ;
